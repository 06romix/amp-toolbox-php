{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    [
      "transform-react-jsx",
      {
        "pragma": "wp.element.createElement"
      }
    ],
    [
      "@wordpress/babel-plugin-makepot",
      {
        "output": "languages/amp-editor-blocks-js.pot"
      }
    ]
  ],
  "env": {
    "default": {
      "plugins": ["transform-runtime"]
    },
    "production": {}
  }
}
